            <form action="POST" enctype="multipart/form-data" id="file">
                <input type="file" accept="image/*" name="photo" class="btn btn-outline-info">
                
                <button id="submit" class="btn btn-outline-primary">Submit</button>
                <h4 id="u-sure"></h4>
            </form>
            <br>
            <img src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" id="preview"
                style="max-width: 250px;">
            <br><br>
            <h4 id="result"></h4>

